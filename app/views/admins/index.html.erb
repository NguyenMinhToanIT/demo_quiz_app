<% provide(:title, "List users")%>
<div class="col-md-2"></div>
<div class="col-md-8">
<h1>List users</h1>
  <table class="table">
    <thead>
      <tr>
        <th>Id</th>
        <th>Avatar</th>
        <th>Name</th>
        <th>Email</th>
        <% if current_user.admin?%>
          <th>Action</th>
        <% end %>
      </tr>
    </thead>
    <tbody>
      <%= render @users%>
    </tbody>
  </table>
  <%= will_paginate @users%>
</div>
<div class="col-md-2"></div>
